<% for (i=0;i< type.length;i++){%>
    <article class="pd-product">
        <img class="pd-img-product" src="/images/img-beer-temporary.jpg" width="360" alt="beer">
        <div class="pd-datos-cerveza">
            <h4><%= type[i].nombre %></h4>
            <h4>$ <%= type[i].precio %></h4>
            <button class="pd-seleccionar"><a href="/product/productDetail/<%= i %>" class="lg-a">Seleccionar</a></button>
            <button class="pd-add"><a href="/product/productCart" class="lg-a">Agregar al carrito</a></button>
            <button class="pd-add"><a href="/product/productAdmin/<%= i %>" class="lg-a">Modificar</a></button>
        </div>
    </article>
    <%}%>